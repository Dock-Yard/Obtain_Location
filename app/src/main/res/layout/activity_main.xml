<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.dockyard.myapplication.MainActivity">

    <TextView
        android:text="Latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:id="@+id/tv1"
        android:textSize="18sp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/tv2"
        android:layout_alignStart="@+id/tv2" />

    <TextView
        android:text="defaultLat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:id="@+id/tv2"
        android:textSize="18sp"
        android:layout_below="@+id/tv1"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv3"
        android:textSize="18sp"
        android:layout_below="@+id/tv2"
        android:layout_alignLeft="@+id/tv2"
        android:layout_alignStart="@+id/tv2"
        android:layout_marginTop="44dp" />

    <TextView
        android:text="defaultLon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:id="@+id/tv4"
        android:textSize="18sp"
        android:layout_alignTop="@+id/tv3"
        android:layout_alignRight="@+id/tv3"
        android:layout_alignEnd="@+id/tv3" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:id="@+id/tv5"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/tv4"
        android:layout_alignEnd="@+id/tv4"
        android:layout_marginBottom="12dp"
        android:id="@+id/tv10"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv10"
        android:layout_alignLeft="@+id/tv10"
        android:layout_alignStart="@+id/tv10"
        android:layout_marginBottom="22dp"
        android:id="@+id/tv9"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv9"
        android:layout_alignLeft="@+id/tv9"
        android:layout_alignStart="@+id/tv9"
        android:layout_marginBottom="27dp"
        android:id="@+id/tv8"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv5"
        android:layout_alignLeft="@+id/tv5"
        android:layout_alignStart="@+id/tv5"
        android:layout_marginTop="20dp"
        android:id="@+id/tv6"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv8"
        android:layout_alignLeft="@+id/tv6"
        android:layout_alignStart="@+id/tv6"
        android:layout_marginBottom="12dp"
        android:id="@+id/tv7"
        android:textSize="18sp" />

</RelativeLayout>
